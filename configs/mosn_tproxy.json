{
	"servers":[
		{
			"default_log_path":"stdout",
			"routers":[
				{
					"router_config_name":"server_router",
					"virtual_hosts":[{
						"name":"serverHost",
						"domains": ["*"],
						"routers": [
							{
								"match":{"prefix":"/"},
								"route":{"cluster_name":"serverCluster"}
							}
						]
					}]
				}
			],
			"listeners":[
				{
					"name":"Listener_16000",
					"address": "127.0.0.1:16000",
                    "bind_port": true,
					"transparent": true,
					"listener_filters": [
						{
							"type": "transparent_proxy",
							"config":{}
						}
					],
					"filter_chains": [
                        {}
                    ]
				},
				{
					"name":"Listener_80",
					"address": "0.0.0.0:80",
					"bind_port": false,
					"filter_chains": [{
						"filters": [
							{
								"type": "proxy",
								"config": {
									"downstream_protocol": "Auto",
									"upstream_protocol": "Auto",
									"router_config_name":"server_router"
								}
							}
						]
					}]
                },
                {
					"name":"Listener_443",
					"address": "0.0.0.0:443",
					"bind_port": false,
					"filter_chains": [{
						"filters": [
							{
								"type": "proxy",
								"config": {
									"downstream_protocol": "Auto",
									"upstream_protocol": "Auto",
									"router_config_name":"server_router"
								}
							}
						]
					}]
				}
			]
		}
	],
	"cluster_manager":{
		"clusters":[
			{
				"name":"serverCluster",
				"type": "SIMPLE",
				"lb_type": "LB_RANDOM",
                "max_request_per_conn": 1024,
                "mark": 68,
				"conn_buffer_limit_bytes":32768,
				"hosts":[
					{"address":"180.101.49.11:80"}
				]
			}
		]
	},
	"admin": {
		"address": {
			"socket_address": {
				"address": "0.0.0.0",
				"port_value": 34901
			}
		}
	}
}
